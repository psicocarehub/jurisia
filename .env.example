# ============================================
# Juris.AI — Environment Variables
# ============================================

# App
APP_NAME=Juris.AI
DEBUG=true
SECRET_KEY=change-me-to-a-secure-random-string
API_KEY_HEADER=X-API-Key

# Database (Supabase)
DATABASE_URL=postgresql+asyncpg://postgres.[PROJECT_REF]:[PASSWORD]@aws-0-sa-east-1.pooler.supabase.com:6543/postgres
SUPABASE_URL=https://[PROJECT_REF].supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
REDIS_URL=redis://localhost:6379/0

# Frontend (Next.js public vars)
NEXT_PUBLIC_SUPABASE_URL=https://[PROJECT_REF].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200
ES_INDEX_PREFIX=jurisai

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=legal_documents

# LLM Providers (atualizado Fev/2026)
GAIA_BASE_URL=http://localhost:8080/v1
GAIA_MODEL_NAME=gaia-legal-reasoning-v1
DEEPSEEK_API_KEY=                      # DeepSeek V3.2 — $0.28/$1.10/M, melhor custo-benefício
QWEN_API_KEY=                          # Qwen 3.5 — $0.50/$2.00/M, Apache 2.0
KIMI_API_KEY=                          # Kimi K2.5 — $0.45/$2.20/M, líder agentic tasks
MINIMAX_API_KEY=                       # MiniMax M2.5 — $0.30/$1.10/M
GOOGLE_API_KEY=                        # Gemini 3 Pro — $1.25/$5.00/M, melhor frontier value
ANTHROPIC_API_KEY=                     # Claude Sonnet 4 — $3/$15/M, complexo
OPENAI_API_KEY=                        # GPT-5.2 — fallback
MARITACA_API_KEY=                      # Sabiá-3 — PT-BR nativo

# Embeddings
VOYAGE_API_KEY=
EMBEDDING_MODEL=voyage-law-2
EMBEDDING_DIM=1024

# OCR
PADDLE_OCR_ENDPOINT=http://localhost:8866
AZURE_DOC_INTELLIGENCE_KEY=
AZURE_DOC_INTELLIGENCE_ENDPOINT=

# Storage (MinIO / S3)
S3_BUCKET=jurisai-documents
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin

# Langfuse
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=http://localhost:3001

# Routing thresholds
ROUTER_THRESHOLD_TIER1=0.6
ROUTER_THRESHOLD_TIER2=0.85

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
API_URL=http://api:8000

# Ingestion — JUIT API (70M decisoes texto integral)
JUIT_API_KEY=
JUIT_API_URL=https://api.juit.dev

# Portal da Transparencia (CEIS/CNEP/CEAF — CC0)
TRANSPARENCIA_API_KEY=

# CNPJ Receita Federal
CNPJ_DATA_DIR=/data/cnpj

# DB Password (used by docker-compose)
DB_PASSWORD=jurisai_dev
SALT=change-me-salt
